  </Target>

  <Target
    Name="InternalPackageNuGet"
    Outputs="$(TmpOutBinOutputPath)%(Projects.Filename).nupkg"
  >
    <PropertyGroup>
      <ProjectsFileName>%(Projects.FileName)</ProjectsFileName>
      <ProjectsConfiguration>%(Projects.Configuration)</ProjectsConfiguration>
      <ProjectsPlatform>%(Projects.Platform)</ProjectsPlatform>
      <ProjectsPackaged>%(Projects.Packaged)</ProjectsPackaged>

      <PackageInputBaseDir Condition="Exists('$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\_PublishedWebsites')">$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\_PublishedWebsites\</PackageInputBaseDir>
      <PackageInputBaseDir Condition="'$(PackageInputBaseDir)'==''">$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\</PackageInputBaseDir>
      <PackageTmpOutDir>$(IntermediateOutputPath)pack\$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\</PackageTmpOutDir>
    </PropertyGroup>
    <ItemGroup>
      <PackagedItems
        Include="$(PackageInputBaseDir)**\*.*"
        Exclude="$(PackageInputBaseDir)**\*.pdb;$(PackageInputBaseDir)**\CodeContracts\**\*.*"
      />
      <SymbolItems Include="$(PackageInputBaseDir)**\*.pdb" Exclude="$(PackageInputBaseDir)**\CodeContracts\**\*.*" />
    </ItemGroup>

    <MakeDir Condition="$(ProjectsPackaged) And Exists('$(ProjectsFileName).nuspec')" Directories="$(PackageTmpOutDir)lib" />
    <Move
      Condition="$(ProjectsPackaged) And Exists('$(ProjectsFileName).nuspec')"
      SourceFiles="@(PackagedItems)"
      DestinationFiles="@(PackagedItems->'$(PackageTmpOutDir)src\%(RecursiveDir)%(Filename)%(Extension)')"
    />
    <Copy
      Condition="$(ProjectsPackaged) And Exists('$(ProjectsFileName).nuspec')"
      SourceFiles="$(ProjectsFileName).nuspec"
      DestinationFiles="$(PackageTmpOutDir)$(ProjectsFileName).nuspec"
      OverwriteReadOnlyFiles="true"
    />

    <MakeDir Condition="$(ProjectsPackaged) And Exists('$(ProjectsFileName).nuspec')" Directories="$(TmpOutBinOutputPath)" />
    <Exec
      Condition="$(ProjectsPackaged) And Exists('$(ProjectsFileName).nuspec')"
      Command="&quot;$(NuGetToolPath)&quot; pack &quot;$(PackageTmpOutDir)$(ProjectsFileName).nuspec&quot; -OutputDirectory &quot;$(TmpOutBinOutputPath).&quot;"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <ItemGroup>
      <NuSpecReferences Remove="@(PackagedItems)" />
      <PackagedItems Remove="@(PackagedItems)" />
      <SymbolItems Remove="@(SymbolItems)" />
    </ItemGroup>
  </Target>

  <Target
    Name="InternalPackageOthers"
    Outputs="$(TmpOutBinOutputPath)%(Projects.Filename)_%(Projects.Platform)_%(Projects.Configuration).zip"
  >
    <PropertyGroup>
      <ProjectsFileName>%(Projects.FileName)</ProjectsFileName>
      <ProjectsConfiguration>%(Projects.Configuration)</ProjectsConfiguration>
      <ProjectsPlatform>%(Projects.Platform)</ProjectsPlatform>
      <ProjectsPackaged>%(Projects.Packaged)</ProjectsPackaged>

      <PackageFileName>$(ProjectsFileName)</PackageFileName>
      <PackageFileName Condition="'%(Projects.Extension)'=='.js'">$(ProjectName)</PackageFileName>
      <PackageBaseName>$(PackageFileName)_$(ProjectsPlatform.Replace(" ", ""))_$(ProjectsConfiguration)_$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)</PackageBaseName>

      <IsWebSite>False</IsWebSite>
      <IsWebSite Condition="Exists('$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\_PublishedWebsites')">True</IsWebSite>
      <PackageInputBaseDir>$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\</PackageInputBaseDir>
      <PackageInputBaseDir Condition="$(IsWebSite)">$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\_PublishedWebsites\</PackageInputBaseDir>
      <IsWebSite Condition="$(IsWebSite) Or Exists('$(TmpBinOutputPath)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\$(PackageFileName)\Web.config')">True</IsWebSite>
      <PackageTmpOutDir>$(IntermediateOutputPath)pack\</PackageTmpOutDir>
      <ProjectPackageTmpOutDir>$(PackageTmpOutDir)$(ProjectsFileName)\$(ProjectsConfiguration)\$(ProjectsPlatform)\</ProjectPackageTmpOutDir>
    </PropertyGroup>

    <ItemGroup>
      <ParamFile Condition="$(IsWebSite)" Include="$(PackageInputBaseDir)*\Parameters.xml" />
      <PackagedItems
        Condition="$(IsWebSite)"
        Include="$(PackageInputBaseDir)*\**\*.*"
        Exclude="@(ParamFile);$(PackageInputBaseDir)**\*.pdb;$(PackageInputBaseDir)**\*.map;$(PackageInputBaseDir)**\CodeContracts\**\*.*"
      />
      <PackagedItems
        Condition="!$(IsWebSite)"
        Include="$(PackageInputBaseDir)**\*.*"
        Exclude="@(ParamFile);$(PackageInputBaseDir)**\*.pdb;$(PackageInputBaseDir)**\*.map;$(PackageInputBaseDir)**\CodeContracts\**\*.*"
      />
      <AllSymbolItems Include="$(PackageInputBaseDir)**\*.pdb" Exclude="$(PackageInputBaseDir)**\CodeContracts\**\*.*" />
    </ItemGroup>

    <FindInList List="@(AllSymbolItems)" ItemSpecToFind="%(CompileOutputs.Filename).pdb" CaseSensitive="False" MatchFileNameOnly="True">
      <Output TaskParameter="ItemFound" ItemName="SymbolItems"/>
    </FindInList>

    <LongPathCopy
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec')"
      SourceFiles="@(PackagedItems)"
      DestinationFiles="@(PackagedItems->'$(PackageTmpOutDir)$(PackageBaseName)\%(RecursiveDir)%(Filename)%(Extension)')"
    />
    <ItemGroup>
      <PackageInputDirItem Condition="$(IsWebSite)" Include="$(PackageTmpOutDir)$(PackageBaseName)\*\Web.config" />
    </ItemGroup>

    <LongPathCopy
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And %(Projects.Library)"
      SourceFiles="@(PackagedItems)"
      DestinationFiles="@(PackagedItems->'$(TmpOutBinOutputPath)$(PackageBaseName)\%(RecursiveDir)%(Filename)%(Extension)')"
    />
    <Zip
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library)"
      Files="@(PackagedItems)"
      ZipFileName="$(TmpOutBinOutputPath)$(PackageBaseName).zip"
      WorkingDirectory="$(PackageInputBaseDir)"
      ZipLevel="9"
    />

    <PropertyGroup>
      <PackageInputDir Condition="'@(PackageInputDirItem)'!=''">@(PackageInputDirItem->'%(RootDir)%(Directory)')</PackageInputDir>
    </PropertyGroup>
    <MakeDir Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)" Directories="$(PackageTmpOutDir)" />
    <Copy
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)"
      SourceFiles="$(BuildToolsPath)\Empty.manifest.xml"
      DestinationFiles="$(PackageTmpOutDir)$(PackageBaseName).manifest.xml"
      OverwriteReadOnlyFiles="true"
    />
    <XmlPoke
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)"
      XmlInputPath="$(PackageTmpOutDir)$(PackageBaseName).manifest.xml"
      Query="/isogeoInstallManifest"
      Value="&lt;contentPath path=&quot;$(PackageInputDir)&quot; /&gt;"
    />

    <PropertyGroup>
      <EscapedPackageInputBaseDir>$(PackageInputDir.Replace("\", "\\"))</EscapedPackageInputBaseDir>
      <DeclareParamFile Condition="'@(ParamFile)'!=''"> -declareParamFile:"@(ParamFile)"</DeclareParamFile>
    </PropertyGroup>
    <MakeDir Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)" Directories="$(ProjectPackageTmpOutDir)" />
    <Exec
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)"
      Command="&quot;$(MSDeployToolPath)&quot; -verb:sync -source:manifest=&quot;$(PackageTmpOutDir)$(PackageBaseName).manifest.xml&quot; -dest:package=&quot;$(ProjectPackageTmpOutDir)package.zip&quot;$(DeclareParamFile) -replace:objectName=path,match=&quot;$(EscapedPackageInputBaseDir)&quot;,replace=&quot;C:\inetpub\$(PackageFileName)\&quot;"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <ItemGroup>
      <InstallFiles Include="$(InputPath)\install\$(PackageFileName)\**\*.*" />
      <InstallFiles Include="$(BuildToolsPath)\7za.exe" />
      <InstallFiles Include="$(InstallToolsPath)\Install.psm1" />
      <InstallFiles Include="$(InstallToolsPath)\psake.ps1" />
      <InstallFiles Include="$(InstallToolsPath)\psake.psm1" />
    </ItemGroup>
    <LongPathCopy
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)"
      SourceFiles="@(InstallFiles)"
      DestinationFiles="@(InstallFiles->'$(ProjectPackageTmpOutDir)%(RecursiveDir)%(Filename)%(Extension)')"
    />
    <ItemGroup>
      <DeployPackagedItems Include="$(ProjectPackageTmpOutDir)**\*.*" />
    </ItemGroup>
    <Zip
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec') And !%(Projects.Library) And $(IsWebSite)"
      Files="@(DeployPackagedItems)"
      ZipFileName="$(TmpOutBinOutputPath)$(PackageBaseName).install.zip"
      WorkingDirectory="$(ProjectPackageTmpOutDir)"
      ZipLevel="9"
    />

    <LongPathCopy
      Condition="$(ProjectsPackaged) And !Exists('$(ProjectsFileName).nuspec')"
      SourceFiles="@(SymbolItems)"
      DestinationFiles="@(SymbolItems->'$(TmpOutPdbOutputPath)$(PackageBaseName)\%(RecursiveDir)%(Filename)%(Extension)')"
    />
    <PropertyGroup>
      <SymbolsStorePath Condition="'$(SymbolsStorePath)'==''">$(TmpOutputPath)symbols\</SymbolsStorePath>
    </PropertyGroup>
    <MakeDir Condition="!Exists('$(SymbolsStorePath)') And $(ProjectsPackaged) And '@(SymbolItems)'!='' And !Exists('$(ProjectsFileName).nuspec')" Directories="$(SymbolsStorePath)" />
    <SymStore
      Condition="$(ProjectsPackaged) And '@(SymbolItems)'!='' And !Exists('$(ProjectsFileName).nuspec')"
      Files="$(TmpOutPdbOutputPath)$(PackageBaseName)"
      Store="$(SymbolsStorePath)"
      Local="True"
      Compress="True"
      Recursive="True"
      Product="$(PackageFileName)"
      Version="$(VersionMajor).$(VersionMinor).$(AutoVersionBuild).$(AutoVersionRevision)"
      ToolPath="$(WindowsSdkDir)Debuggers\x64"
    />
    <ItemGroup>
      <PackagedItems Remove="@(PackagedItems)" />
      <SymbolItems Remove="@(SymbolItems)" />
    </ItemGroup>
  </Target>

  <Target Name="BeforePackage" />
  <Target Name="AfterPackage" />



  <Target
    Name="UpdateVersion"
    Condition="'$(CCNetLabel)'!=''"
    DependsOnTargets="InternalUpdateVersion;InternalUpdateVersionNetFiles;InternalUpdateVersionJsFiles;InternalUpdateVersionNuSpecFiles"
  />

  <Target Name="InternalUpdateVersion">
    <VersionInfo Version="$(CCNetLabel)">
      <Output TaskParameter="Major" PropertyName="VersionMajor" />
      <Output TaskParameter="Minor" PropertyName="VersionMinor" />
      <Output TaskParameter="Build" PropertyName="VersionBuild" />
      <Output TaskParameter="Revision" PropertyName="VersionRevision" />
    </VersionInfo>

    <Version Major="$(VersionMajor)" Minor="$(VersionMinor)" RevisionType="Automatic" BuildType="Automatic">
      <Output TaskParameter="Build" PropertyName="AutoVersionBuild" />
      <Output TaskParameter="Revision" PropertyName="AutoVersionRevision" />
    </Version>
  </Target>

  <Target
    Name="InternalUpdateVersionNetFiles"
    Condition="Exists('Version.cs')"
  >
    <FileUpdate
      Files="Version.cs"
      Regex="(?&lt;=AssemblyVersion\W{2})[\d\.]+(?=\W{2})"
      ReplacementText="$(VersionMajor).$(VersionMinor).$(AutoVersionBuild).$(AutoVersionRevision)"
    />
    <FileUpdate
      Files="Version.cs"
      Regex="(?&lt;=AssemblyInformationalVersion\W{2})[\d\.]+(?=\W{2})"
      ReplacementText="$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)"
    />
    <FileUpdate
      Files="Version.cs"
      Regex="(?&lt;=AssemblyFileVersion\W{2})[\d\.]+(?=\W{2})"
      ReplacementText="$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)"
    />
  </Target>

  <Target
    Name="InternalUpdateVersionJsFiles"
    Condition="Exists('package.json')"
  >
    <FileUpdate
      Files="package.json"
      Encoding="Windows-1252"
      Regex="(?&lt;=&quot;version&quot;\W\s+&quot;)\d+\.\d+\.\d+\W\d+(?=&quot;)"
      ReplacementText="$(VersionMajor).$(VersionMinor).$(VersionBuild)-$(VersionRevision)"
    />
  </Target>

  <Target
    Name="InternalUpdateVersionNuSpecFiles"
    Outputs="%(Projects.Filename).nuspec"
  >
    <XmlUpdate
      Condition="Exists('%(Projects.Filename).nuspec')"
      XmlFileName="%(Projects.Filename).nuspec"
      Namespace="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd"
      Prefix="ns"
      XPath="/ns:package/ns:metadata/ns:version"
      Value="$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)"
    />
  </Target>

  

  <!-- Commits the changes -->
  <Target
    Name="CommitChanges"
    Condition="'$(BuildType)'=='Release' And '$(CCNetLabel)'!=''"
    DependsOnTargets="UpdateVersion;InternalGitCommitChanges"
  />

  <Target
    Name="InternalGitCommitChanges"
    Condition="'$(BuildType)'=='Release' And '$(CCNetLabel)'!='' And Exists('$(InputPath)\.git\index') And '$(GitToolPath)'!=''"
  >
    <Exec
      Command="&quot;$(GitToolPath)&quot; commit -a -u -m &quot;Update version number to $(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)&quot; --signoff"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <Exec
      Command="&quot;$(GitToolPath)&quot; checkout ."
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <Exec
      Command="&quot;$(GitToolPath)&quot; tag -a &quot;v$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)&quot; -m &quot;Release build v$(VersionMajor).$(VersionMinor).$(VersionBuild).$(VersionRevision)&quot;"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <Exec
      Command="&quot;$(GitToolPath)&quot; push origin HEAD:refs/heads/$(GitBranchName)"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
    <Exec
      Command="&quot;$(GitToolPath)&quot; push --tags"
      WorkingDirectory="$(InputPath)"
      YieldDuringToolExecution="True"
    />
  </Target>
</Project>
